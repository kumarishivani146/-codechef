#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main(){
    int t,n,k;
    //long long int count=0;
    cin>>t;
    while(t--){
        cin>>n;
        unordered_map<int,bool> m;
        vector<int> v;
        while(n){
            cin>>k;
            if(!m[k]&&k!=0)
            {
                v.push_back(k);
                m[k]=true;
            }
            n--;
        }
        cout<<v.size()<<endl;

    }
return 0;
}
